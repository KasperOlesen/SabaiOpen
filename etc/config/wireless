
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'pci0000:00/0000:00:1c.5/0000:06:00.0'
	option htmode 'HT20'
	option country 'US'
	option channel 'auto'
	option disabled '0'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'WmSabaiOpen'
	option encryption 'psk2+aes'
	option key 'sabaipass123'

