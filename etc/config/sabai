
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth1 eth2 eth3'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.199.1'
	option netmask '255.255.255.0'

config interface 'wan'
	option ifname 'eth0'
	option mtu '1500'
	option netmask '255.255.255.0'
	option dns '8.8.8.8 4.2.2.2 8.8.4.4 198.6.1.4'
	option mac '00:30:18:af:3a:24'
	option ipaddr '192.168.200.254'
	option gateway '192.168.200.1'
	option proto 'dhcp'

config interface 'vpn'
	option ifname 'pptp-vpn'
	option proto 'pptp'
	option buffering '1'
	option status 'none'

config settings 'general'
	option ovpn_status 'stopped'
	option ovpn_file '/etc/sabai/openvpn/ovpn.current'

config interface 'dns'
	option servers '8.8.8.8 4.2.2.2 8.8.4.4 198.6.1.4'

config interface 'dhcp'
	option lan 'yes'
	option leasetime '24'
	option start '100'
	option limit '199'
	option table '{"aaData": [{"ip": "192.168.200.254", "mac": "00:30:18:AF:3A:24", "name": "eth0", "time": "----"}, {"ip": "192.168.199.128", "mac": "08:10:76:2a:5c:6e", "name": "wmofficedesktop", "time": "Thu Sep 11 09:50:55 EDT 2014"}, {"ip": "192.168.199.177", "mac": "00:24:2c:49:8d:40", "name": "compaq-Compaq-Presario-CQ60-Notebook-PC", "time": "Thu Sep 11 09:52:18 EDT 2014"}, {"ip": "192.168.199.157", "mac": "00:1f:16:e1:4a:ba", "name": "*", "time": "Thu Sep 11 09:51:03 EDT 2014"}, {"ip": "192.168.199.137", "mac": "8c:3a:e3:73:73:80", "name": "android-995ed86a74557780", "time": "Thu Sep 11 11:02:13 EDT 2014"}]}'

config settings 'time'
	option servers '0.openwrt.pool.ntp.org 3.openwrt.pool.ntp.org 2.openwrt.pool.ntp.org 1.openwrt.pool.ntp.org'
	option location 'America/New_York'
	option timezone 'EST5EDT,M3.2.0,M11.1.0'

config settings 'firewall'
	option cookies 'on'
	option multicast 'on'
	option wanroute 'on'
	option icmp 'on'

config settings 'dmz'
	option destination '192.168.199.248'

config settings 'pf'
	option json 'test'
	option table '{"aaData":[{"status":"on","protocol":"Both","gateway":"WAN","src":"24.24.24.24","ext":"15","int":"56","address":"192.168.199.2","description":"Test Data"},{"status":"on","protocol":"Both","gateway":"WAN","src":"24.24.24.25","ext":"16","int":"57","address":"192.168.199.2","description":"Test Data2"},{"status":"on","protocol":"Both","gateway":"WAN","src":"24.24.24.26","ext":"17","int":"58","address":"192.168.199.2","description":"Test Data3"},{"status":"on","protocol":"Both","gateway":"WAN","src":"24.24.24.274","ext":"18","int":"59","address":"192.168.199.2","description":"Test Data4"}]}'

config settings 'wlradio0'
	option mode 'ap'
	option ssid 'SabaiOpen'
	option wpa_psk 'sabaipass123'
	option wpa_rekey '3600'
	option wepkeys '0123456789 abcdef1234 8649634072 9898989898'
	option wpa_encryption 'aes'
	option encryption 'psk2'

config settings 'wlradio1'
	option type 'server'
	option ssid 'SabaiOpen'
	option security 'wpapersonal'
	option wpa_type '3'
	option wpa_encryption '3'
	option wpa_psk 'sabaipass123'
	option wpa_rekey '3600'
	option wepkeys '0123456789 abcdef1234 9898989898 8649634072'

